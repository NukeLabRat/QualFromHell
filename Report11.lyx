\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\alpha_{1}}{r}\frac{\partial}{\partial r}\left(r\frac{\partial\Theta_{1}}{\partial r}\right)=\frac{\partial\Theta_{1}\left(r,t\right)}{\partial t}\;for\;0\leq r\leq R_{F},\:t>0\label{eq:HeatEqFuel}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\alpha_{2}}{r}\frac{\partial}{\partial r}\left(r\frac{\partial\Theta_{2}}{\partial r}\right)=\frac{\partial\Theta_{2}\left(r,t\right)}{\partial t}\;for\;R_{F}<r<R_{G},\:t>0\label{eq:HeatEqGap}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\alpha_{3}}{r}\frac{\partial}{\partial r}\left(r\frac{\partial\Theta_{3}}{\partial r}\right)=\frac{\partial\Theta_{3}\left(r,t\right)}{\partial t}\;for\;R_{G}<r<R_{C},\:t>0\label{eq:HeatEqClad}
\end{equation}

\end_inset


\end_layout

