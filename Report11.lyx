\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
\Theta_{3}\left(r,t\right)=F_{3}\left(r\right) & for & t=0,\;R_{G}<r<R_{C}\label{eq:IC3}
\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{3}\left(r\right)=\frac{\Theta_{3}\left(R_{G},0\right)-\Theta_{3}\left(R_{C},0\right)}{\ln\left(\frac{R_{G}}{R_{C}}\right)}\ln\left(\frac{r}{R_{C}}\right)+\Theta_{3}\left(R_{C},0\right)
\]

\end_inset


\end_layout

