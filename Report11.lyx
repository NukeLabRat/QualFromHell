\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{1}{r}\frac{d}{dr}\left(r\frac{dT_{1}}{dr}\right)+\frac{q^{\prime\prime\prime}}{k_{1}}=0\;for\;0\leq r\leq R_{F}\label{eq:HeatEqFueSS}
\end{equation}

\end_inset


\begin_inset CommandInset nomenclature
LatexCommand nomenclature
prefix "qppp"
symbol "$q^{\\prime\\prime\\prime}$"
description "Volumetric heat generation $\\left[\\nicefrac{W}{m^3}\\right]$"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{dT}{dr}\Bigg|_{r=0}=0\label{eq:FuelSymmBoundary}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
T_{1}\left(R_{F}\right)=q^{\prime}\left[\frac{1}{2\pi R_{C}h}+\frac{\ln\left(\nicefrac{R_{C}}{R_{G}}\right)}{2\pi k_{3}}+\frac{\ln\left(\nicefrac{R_{G}}{R_{F}}\right)}{2\pi k_{2}}\right]+T_{Coolant}\label{eq:FuelTempBoundary}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
end{subequations}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
r\frac{dT_{1}}{dr}=-\frac{q^{\prime}}{2k_{1}}r^{2}+C_{1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
T_{1}\left(r\right)=-\frac{q^{\prime}}{4k_{1}}r^{2}+C_{1}\ln\left(r\right)+C_{2}\label{eq:IntegratedT1}
\end{equation}

\end_inset


\begin_inset CommandInset nomenclature
LatexCommand nomenclature
prefix "T"
symbol "T"
description "Temperature $K$"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{1}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{2}=T_{1}\left(R_{F}\right)+\frac{q^{\prime}}{4k_{1}}R_{F}^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
T_{1}\left(r\right)=\frac{q^{\prime}}{4k_{1}}\left(R_{F}^{2}-r^{2}\right)+T_{1}\left(R_{F}\right)
\end{equation}

\end_inset


\begin_inset CommandInset nomenclature
