\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial^{2}T(r,t)}{\partial r^{2}}+\frac{1}{r}\frac{\partial T(r,t)}{\partial r}+\frac{1}{k}g(t)=\frac{1}{\alpha}\frac{\partial T\left(r,t\right)}{\partial t}\label{eq:CylinderDiffHeatEq-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial^{2}\theta_{i}(r,t)}{\partial r^{2}}+\frac{1}{r}\frac{\partial\theta_{i}(r,t)}{\partial r}+\frac{1}{k_{i}}g(t)=\frac{1}{\alpha_{i}}\frac{\partial\theta_{i}\left(r,t\right)}{\partial t}\label{eq:CylinderDiffHeatEqGen}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\begin{array}{c}
\theta_{i}\left(r,t\right)=\overset{3}{\underset{j=1}{\sum}}\left[\int_{r^{\prime}=r_{j}}^{r^{\prime}=r_{j+1}}r^{\prime}G_{ij}\left(r,t\left|r^{\prime},\tau\right.\right)\left|_{\tau=0}\right.F_{j}\left(r^{\prime}\right)dr^{\prime}\right]\\
\hphantom{+\int_{r^{\prime}=r_{j}}^{r^{\prime}=r_{j+1}}}+\frac{\alpha_{i}}{k_{i}}\int_{\tau=0}^{\tau=t}\left[\overset{3}{\underset{j=1}{\sum}}\left[\int_{r^{\prime}=r_{j}}^{r^{\prime}=r_{j+1}}r^{\prime}G_{ij}\left(r,t\left|r^{\prime},\tau\right.\right)g_{j}\left(r^{\prime},\tau\right)dr^{\prime}\right]\right]d\tau
\end{array}\label{eq:ThetaGreensFunGen}
\end{equation}

\end_inset


